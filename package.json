{
  "name": "import-organizer",
  "displayName": "Import Organizer",
  "description": "Organize import statements",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Disable imports sorting",
        "category": "Imports Organizer",
        "command": "import-organizer.disable"
      },
      {
        "title": "Enable imports sorting",
        "category": "Imports Organizer",
        "command": "import-organizer.enable"
      }
    ],
    "configuration": {
      "title": "Imports Organizator",
      "properties": {
        "import-organizer.mixType": {
          "type": "string",
          "default": "es_to_top",
          "enum": [
            "mixed",
            "es_to_top"
          ],
          "enumDescriptions": [
            "CommonJS and Es imports are mixed, sorting and grouping are done based on sortOrder config",
            "At first es imports are grouped and sorted based on sortOrder config, and then commonJS"
          ]
        },
        "import-organizer.sortOrder": {
          "type": "array",
          "scope": "resource",
          "description": "List of sorting rules.",
          "default": [
            {
              "order": 1,
              "test": "package",
              "internalOrder": [
                "core",
                "^react$",
                "^@.+",
                "^react"
              ]
            },
            {
              "order": 4,
              "test": "^\\.[a-zA-Z]+$"
            },
            {
              "order": 2,
              "test": "parent"
            },
            {
              "order": 3,
              "test": "sibling"
            },
            {
              "test": "all",
              "internalOrder": [
                "image",
                "style",
                "\\.atoms\\."
              ]
            }
          ],
          "items": {
            "anyOf": [
              {
                "type": "object",
                "properties": {
                  "order": {
                    "type": "number",
                    "description": "Applied order of element"
                  },
                  "test": {
                    "type": "string",
                    "description": "regexp"
                  },
                  "internalOrder": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            ]
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/babel-traverse": "^6.25.4",
    "@types/babel-types": "^7.0.4",
    "@types/glob": "^7.1.1",
    "@types/jest": "^26.0.3",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "jest": "^26.1.0",
    "mocha": "^7.1.2",
    "ts-jest": "^26.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.10.3",
    "@babel/parser": "^7.10.3",
    "@babel/template": "^7.10.3",
    "@babel/traverse": "^7.10.3",
    "@babel/types": "^7.10.3",
    "@types/babel__core": "^7.1.9",
    "@types/babel__traverse": "^7.0.12"
  }
}
